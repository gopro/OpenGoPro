[darglint]
message_template={msg_id} @ {path}:{line} ==> {msg}
docstring_style=google
enable= DAR001,
    DAR002,
    DAR003,
    DAR004,
    DAR005,
    DAR101,
    DAR102,
    DAR103,
    DAR104,
    DAR105,
    DAR201,
    DAR203,
    DAR301,
    DAR302,
    DAR401,
    DAR402,
    DAR501
# This breaks on methods generated from decorators so we allow pylint to handle excess returns
ignore=DAR202
